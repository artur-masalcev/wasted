<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <ProduceReferenceAssembly>true</ProduceReferenceAssembly>
  </PropertyGroup>

  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Debug|AnyCPU'">
    <DebugType>portable</DebugType>
    <DebugSymbols>true</DebugSymbols>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Xamarin.Forms" Version="5.0.0.2012" />
    <PackageReference Include="Xamarin.Essentials" Version="1.6.1" />
    <PackageReference Include="Newtonsoft.Json" Version="13.0.1" />
  </ItemGroup>
  <ItemGroup>
    <None Remove="Dummy Data API\" />
    <None Remove="Dummy Data API\Dummy Data\" />
    <None Remove="Dummy Data API\Dummy Data\Deals.json" />
    <None Remove="Dummy Data API\Dummy Data\Providers.json" />
    <None Remove="Images\outline_home_black_24dp.png" />
    <None Remove="Images\outline_person_black_24dp.png" />
    <None Remove="Images\outline_receipt_long_black_24dp.png" />
    <None Remove="Images\outline_search_black_24dp.png" />
    <None Remove="Utils\" />
    <None Remove="Dummy API\Business Objects\" />
    <None Remove="Dummy API\Dummy Data\FoodPlaces.json" />
    <None Remove="Newtonsoft.Json" />
  </ItemGroup>
  <ItemGroup>
    <Folder Include="Dummy API\" />
    <Folder Include="Dummy API\Dummy Data\" />
    <Folder Include="Utils\" />
    <Folder Include="Dummy API\Business Objects\" />
  </ItemGroup>
  <ItemGroup>
    <EmbeddedResource Include="Dummy API\Dummy Data\Deals.json" />
    <EmbeddedResource Include="Dummy API\Dummy Data\FoodPlaces.json" />
    <EmbeddedResource Include="Images\Icons\outline_home_black_24dp.png" />
    <EmbeddedResource Include="Images\Icons\outline_person_black_24dp.png" />
    <EmbeddedResource Include="Images\Icons\outline_receipt_long_black_24dp.png" />
    <EmbeddedResource Include="Images\Icons\outline_search_black_24dp.png" />
  </ItemGroup>
  <ItemGroup>
    <EmbeddedResource Update="HomePage.xaml">
      <Generator>MSBuild:UpdateDesignTimeXaml</Generator>
    </EmbeddedResource>
    <EmbeddedResource Update="OrdersPage.xaml">
      <Generator>MSBuild:UpdateDesignTimeXaml</Generator>
    </EmbeddedResource>
    <EmbeddedResource Update="ProfilePage.xaml">
      <Generator>MSBuild:UpdateDesignTimeXaml</Generator>
    </EmbeddedResource>
    <EmbeddedResource Update="SearchPage.xaml">
      <Generator>MSBuild:UpdateDesignTimeXaml</Generator>
    </EmbeddedResource>
  </ItemGroup>
</Project>